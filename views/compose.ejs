<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="POST">
    <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Post</label>
        <textarea name="postBody" class="form-control" cols="30" rows="5"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary" value="">Publish</button>

</form>

<%- include('partials/footer') -%>